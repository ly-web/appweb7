DEPS_13 = $(DEPS_13) $(CONFIG)\obj\estLib.obj
!IF "$(BIT_PACK_EST)" == "1"
DEPS_13 = $(DEPS_13) $(CONFIG)\bin\libest.lib
!ENDIF
