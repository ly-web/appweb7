build\$(CONFIG)\inc\http.h: $(DEPS_15)
	@echo ..... [Copy] build\$(CONFIG)\inc\http.h
	if not exist "build\$(CONFIG)\inc" md "build\$(CONFIG)\inc"
	copy /Y /B src\http\http.h+modified build\$(CONFIG)\inc\http.h $(LOG)

#
#   http.h
#

src\http\http.h: $(DEPS_16)

#
#   httpLib.obj
#
DEPS_17 = $(DEPS_17) src\http\http.h
DEPS_17 = $(DEPS_17) build\$(CONFIG)\inc\pcre.h

build\$(CONFIG)\obj\httpLib.obj: \
    src\http\httpLib.c $(DEPS_17)
	@echo .. [Compile] build\$(CONFIG)\obj\httpLib.obj
	"$(CC)" -c $(DFLAGS) -Fo$(BUILD)\obj\httpLib.obj -Fd$(BUILD)\obj\httpLib.pdb $(CFLAGS) -DME_COM_OPENSSL_PATH=$(ME_COM_OPENSSL_PATH) $(IFLAGS) "-I$(ME_COM_OPENSSL_PATH)\include" src\http\httpLib.c $(LOG)

!IF "$(ME_COM_HTTP)" == "1"
#
#   libhttp
#
DEPS_18 = $(DEPS_18) build\$(CONFIG)\bin\libmpr.dll
!IF "$(ME_COM_PCRE)" == "1"
DEPS_18 = $(DEPS_18) build\$(CONFIG)\bin\libpcre.dll
!ENDIF
DEPS_18 = $(DEPS_18) build\$(CONFIG)\inc\http.h
DEPS_18 = $(DEPS_18) build\$(CONFIG)\obj\httpLib.obj

!IF "$(ME_COM_OPENSSL)" == "1"
LIBS_18 = $(LIBS_18) libmpr-openssl.lib
!ENDIF
!IF "$(ME_COM_OPENSSL)" == "1"
LIBS_18 = $(LIBS_18) libgdi32.lib
LIBPATHS_18 = $(LIBPATHS_18) -libpath:$(ME_COM_OPENSSL_PATH)
!ENDIF
!IF "$(ME_COM_OPENSSL)" == "1"
LIBS_18 = $(LIBS_18) libeay32.lib
LIBPATHS_18 = $(LIBPATHS_18) -libpath:$(ME_COM_OPENSSL_PATH)
!ENDIF
!IF "$(ME_COM_OPENSSL)" == "1"
LIBS_18 = $(LIBS_18) libssleay32.lib
LIBPATHS_18 = $(LIBPATHS_18) -libpath:$(ME_COM_OPENSSL_PATH)
!ENDIF
LIBS_18 = $(LIBS_18) libmpr.lib
!IF "$(ME_COM_PCRE)" == "1"
LIBS_18 = $(LIBS_18) libpcre.lib
!ENDIF
!IF "$(ME_COM_PCRE)" == "1"
LIBS_18 = $(LIBS_18) libpcre.lib
!ENDIF
LIBS_18 = $(LIBS_18) libmpr.lib

build\$(CONFIG)\bin\libhttp.dll: $(DEPS_18)
	@echo ..... [Link] build\$(CONFIG)\bin\libhttp.dll
	"$(LD)" -dll -out:$(BUILD)\bin\libhttp.dll -entry:$(ENTRY) $(LDFLAGS) $(LIBPATHS)  "$(BUILD)\obj\httpLib.obj" $(LIBPATHS_18) $(LIBS_18) $(LIBS_18) $(LIBS)  $(LOG)
!ENDIF

#
